{
    "scopeName": "source.tcss",
    "patterns": [{ "include": "#style-block" }],
    "repository": {
        "style-block": {
            "begin": "\\w+ *{",
            "end": "}",
            "patterns": [
                { "include": "#rule-definition" }
            ]
        },
        "rule-definition": {
            "patterns": [
                { "include": "#border-rule" },
                { "include": "#color-rule" },
                { "include": "#horizontal-vertical-rule" },
                { "include": "#horizontal-rule" },
                { "include": "#vertical-rule" },
                { "include": "#scalar-rule" },
                { "include": "#text-style-rule" },
                { "include": "#box-sizing-rule" },
                { "include": "#display-rule" },
                { "include": "#dock-rule" },
                { "include": "#integer-rule" },
                { "include": "#multiple-scalars-rule" },
                { "include": "#grid-gutter-rule" },
                { "include": "#grid-size-rule" },
                { "include": "#layout-rule" },
                { "include": "#overflow-rule" },
                { "include": "#overflow-dir-rule" },
                { "include": "#scrollbar-gutter-rule" },
                { "include": "#text-align-rule" },
                { "include": "#visibility-rule" },
                { "include": "#layer-rule" },
                { "include": "#layers-rule" },
                { "include": "#integers-1-2-4-rule" },
                { "include": "#offset-rule" },
                { "include": "#opacity-rule" },
                { "include": "#scrollbar-size-rule" },
                { "include": "#invalid-rule-name" }
            ]
        },
        "border-rule": {
            "begin": "\\s*(border|border-top|border-right|border-bottom|border-left|outline|outline-top|outline-right|outline-bottom|outline-left)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            },
            "patterns": [
                { "include": "#border-color-value" },
                { "include": "#border-value" },
                { "include": "#invalid-value" }
            ]
        },
        "color-rule": {
            "begin": "\\s*(background|border-subtitle-background|border-subtitle-color|border-title-color|border-title-background|color|link-background|link-color|link-hover-background|link-hover-color|scrollbar-background|scrollbar-background-active|scrollbar-background-hover|scrollbar-color|scrollbar-color-active|scrollbar-color-hover|scrollbar-corner-color|tint)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            },
            "patterns": [
                { "include": "#color-value" },
                { "include": "#invalid-value" }
            ]
        },
        "horizontal-vertical-rule": {
            "begin": "\\s*(align|content-align)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#horizontal-vertical-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "horizontal-rule": {
            "begin": "\\s*(align-horizontal|border-subtitle-align|border-title-align|content-align-horizontal)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#horizontal-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "vertical-rule": {
            "begin": "\\s*(align-vertical|content-align-vertical)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#vertical-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "scalar-rule": {
            "begin": "\\s*(height|max-height|max-width|min-height|min-width|offset-x|offset-y|width)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#scalar-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "text-style-rule": {
            "begin": "\\s*(border-subtitle-style|border-title-style|link-hover-style|link-style|text-style)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#text-style-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "box-sizing-rule": {
            "begin": "\\s*(box-sizing)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "border-box|content-box",
                    "name": "support.constant.property-value"
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "display-rule": {
            "begin": "\\s*(display)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "block|none",
                    "name": "support.constant.property-value"
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "dock-rule": {
            "begin": "\\s*(dock)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "bottom|left|right|top",
                    "name": "support.constant.property-value"
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "integer-rule": {
            "begin": "\\s*(column-span|row-span|margin-top|margin-right|margin-bottom|margin-left|padding-top|padding-right|padding-bottom|padding-left|scrollbar-size-horizontal|scrollbar-size-vertical)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#integer-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "multiple-scalars-rule": {
            "begin": "\\s*(grid-columns|grid-rows)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#multiple-scalars-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "grid-gutter-rule": {
            "begin": "\\s*(grid-gutter)\\s*:\\s",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "(\\S+)(\\s+(\\S+))?",
                    "captures": {
                        "1": {
                            "patterns": [
                                { "include": "#scalar-value" },
                                { "include": "#invalid-value" }
                            ]
                        },
                        "3": {
                            "patterns": [
                                { "include": "#scalar-value" },
                                { "include": "#invalid-value" }
                            ]
                        }
                    }
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "grid-size-rule": {
            "begin": "\\s*(grid-size)\\s*:\\s",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "(\\S+)(\\s+(\\S+))?",
                    "captures": {
                        "1": {
                            "patterns": [
                                { "include": "#integer-value" },
                                { "include": "#invalid-value" }
                            ]
                        },
                        "3": {
                            "patterns": [
                                { "include": "#integer-value" },
                                { "include": "#invalid-value" }
                            ]
                        }
                    }
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "layout-rule": {
            "begin": "\\s*(layout)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "grid|horizontal|vertical",
                    "name": "support.constant.property-value"
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "overflow-rule": {
            "begin": "\\s*(overflow)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "(\\S+)\\s+(\\S+)",
                    "captures": {
                        "1": {
                            "patterns": [
                                { "include": "#overflow-value" },
                                { "include": "#invalid-value" }
                            ]
                        },
                        "2": {
                            "patterns": [
                                { "include": "#overflow-value" },
                                { "include": "#invalid-value" }
                            ]
                        }
                    }
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "overflow-dir-rule": {
            "begin": "\\s*(overflow-x|overflow-y)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#overflow-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "scrollbar-gutter-rule": {
            "begin": "\\s*(scrollbar-gutter)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "auto|stable",
                    "name": "support.constant.property-value"
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "text-align-rule": {
            "begin": "\\s*(text-align)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "center|end|justify|left|right|start",
                    "name": "support.constant.property-value"
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "visibility-rule": {
            "begin": "\\s*(visibility)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "hidden|visible",
                    "name": "support.constant.property-value"
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "layer-rule": {
            "begin": "\\s*(layer)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#name-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "layers-rule": {
            "begin": "\\s*(layers)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#multiple-names-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "integers-1-2-4-rule": {
            "begin": "\\s*(margin|padding)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#integers-1-2-4-value" },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "offset-rule": {
            "begin": "\\s*(offset)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "(\\S+)\\s+(\\S+)",
                    "captures": {
                        "1": {
                            "patterns": [
                                { "include": "#scalar-value" },
                                { "include": "#invalid-value" }
                            ]
                        },
                        "2": {
                            "patterns": [
                                { "include": "#scalar-value" },
                                { "include": "#invalid-value" }
                            ]
                        }
                    }
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "scrollbar-size-rule": {
            "begin": "\\s*(scrollbar-size)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                {
                    "match": "(\\S+)\\s+(\\S+)",
                    "captures": {
                        "1": {
                            "patterns": [
                                { "include": "#integer-value" },
                                { "include": "#invalid-value" }
                            ]
                        },
                        "2": {
                            "patterns": [
                                { "include": "#integer-value" },
                                { "include": "#invalid-value" }
                            ]
                        }
                    }
                },
                { "include": "#invalid-value" }
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "opacity-rule": {
            "begin": "\\s*(opacity|text-opacity)\\s*:\\s*",
            "end": "\\s*(!important)\\s*;",
            "patterns": [
                { "include": "#percentage" },
                { "include": "#decimal-percentage" },
                { "include": "#invalid-value"}
            ],
            "beginCaptures": {
                "1": { "name": "support.type.property-name" }
            },
            "endCaptures": {
                "1": { "name": "keyword.other.important" }
            }
        },
        "integers-1-2-4-value": {
            "match": "(\\d+)(\\s+(\\d+)(\\s+(\\d+)\\s+(\\d+))?)?",
            "captures": {
                "1": {
                    "patterns": [
                        { "include": "#integer-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "3": {
                    "patterns": [
                        { "include": "#integer-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "5": {
                    "patterns": [
                        { "include": "#integer-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "6": {
                    "patterns": [
                        { "include": "#integer-value" },
                        { "include": "#invalid-value" }
                    ]
                }
            }
        },
        "multiple-names-value": {
            "match": "(\\S+)\\s+(.*)|(\\S+)",
            "captures": {
                "1": {
                    "patterns": [
                        { "include": "#name-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "2": {
                    "patterns": [
                        { "include": "#recursive-names-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "3": {
                    "patterns": [
                        { "include": "#name-value" },
                        { "include": "#invalid-value" }
                    ]
                }
            }
        },
        "recursive-names-value": {
            "match": "(\\S+)(\\s+(.*))|(\\S+)",
            "captures": {
                "1": {
                    "patterns": [
                        { "include": "#name-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "3": {
                    "patterns": [
                        { "include": "#recursive-names-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "4": {
                    "patterns": [
                        { "include": "#name-value" },
                        { "include": "#invalid-value" }
                    ]
                }
            }
        },
        "name-value": {
            "match": "[a-zA-Z_][a-zA-Z\\d_-]*",
            "name": "support.constant.property-value"
        },
        "overflow-value": {
            "match": "auto|hidden|scroll",
            "name": "support.constant.property-value"
        },
        "multiple-scalars-value": {
            "match": "(\\S+)\\s+(.*)|(\\S+)",
            "captures": {
                "1": {
                    "patterns": [
                        { "include": "#scalar-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "2": {
                    "patterns": [
                        { "include": "#recursive-scalars-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "3": {
                    "patterns": [
                        { "include": "#scalar-value" },
                        { "include": "#invalid-value" }
                    ]
                }
            }
        },
        "recursive-scalars-value": {
            "match": "(\\S+)(\\s+(.*))|(\\S+)",
            "captures": {
                "1": {
                    "patterns": [
                        { "include": "#scalar-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "3": {
                    "patterns": [
                        { "include": "#recursive-scalars-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "4": {
                    "patterns": [
                        { "include": "#scalar-value" },
                        { "include": "#invalid-value" }
                    ]
                }
            }
        },
        "integer-value": {
            "match": "\\d+",
            "name": "constant.numeric"
        },
        "text-style-value": {
            "match": "none|(bold|italic|reverse|strike|underline)(\\s+(bold|italic|reverse|strike|underline))*",
            "name": "support.constant.property-value"
        },
        "scalar-value": {
            "match": "auto|(\\d+[.]?\\d*|\\d*[.]\\d+)(fr|vw|vh|h|w|%)|\\d+",
            "name": "constant.numeric"
        },
        "horizontal-vertical-value": {
            "match": "(\\w+)\\s+(\\w+)",
            "captures": {
                "1": {
                    "patterns": [
                        { "include": "#horizontal-value" },
                        { "include": "#invalid-value" }
                    ]
                },
                "2": {
                    "patterns": [
                        { "include": "#vertical-value" },
                        { "include": "#invalid-value" }
                    ]
                }
            }
        },
        "horizontal-value": {
            "match": "\\b(center|left|right)\\b",
            "name": "support.constant.property-value"
        },
        "vertical-value": {
            "match": "\\b(top|middle|bottom)\\b",
            "name": "support.constant.property-value"
        },
        "border-color-value": {
            "match": "(\\w+)\\s+(.*)",
            "captures": {
                "1": {
                    "patterns": [{ "include": "#border-value" }]
                },
                "2": {
                    "patterns": [{ "include": "#color-value" }]
                }
            }
        },
        "color-percentage-value": {
            "match": "(.*)\\s+(\\d+[.]?\\d*%|\\d*[.]\\d+%)",
            "captures": {
                "1": {
                    "patterns": [{ "include": "#color-value" }]
                },
                "2": { "name": "constant.numeric" }
            }
        },
        "color-value": {
            "patterns": [
                { "include": "#auto-value" },
                { "include": "#color-value-named-color" },
                { "include": "#color-value-hex" },
                { "include": "#color-value-rgb-description" },
                { "include": "#color-value-rgba-description" },
                { "include": "#color-value-hsl-description" },
                { "include": "#color-value-hsla-description" }
            ]
        },
        "auto-value": {
            "match": "(auto)(\\s+(.+))?",
            "captures": {
                "1": { "name": "support.constant.property-value" },
                "3": {
                    "patterns": [
                        { "include": "#percentage" },
                        { "include": "#invalid-value" }
                    ]
                }
            }
        },
        "color-value-named-color": {
